<!-- *************** US REALM HEADER (V4) *************** -->
<ClinicalDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="urn:hl7-org:v3"
    xmlns:sdtc="urn:hl7-org:sdtc"
    xmlns:voc="urn:hl7-org:v3/voc">
    <realmCode code="US"/>
